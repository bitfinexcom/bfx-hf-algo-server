{
  "name": "bfx-hf-algo-server",
  "version": "1.4.3",
  "description": "HF Algorithmic Order Server",
  "main": "index.js",
  "engines": {
    "node": ">=8.3.0"
  },
  "directories": {
    "lib": "lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)",
    "Anton Nazarenko <anton@code-care.pro>"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "lint": "./node_modules/.bin/eslint lib/ examples/ test/ index.js",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test nyc --check-coverage --lines 85 --branches 70 --functions 80 --statements 80 --reporter=lcov --reporter=html mocha -b --recursive",
    "start": "node examples/server.js",
    "docs": "rm docs/reference.md && ./node_modules/.bin/jsdoc2md -f index.js 'lib/**/*.js' > docs/reference.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-algo-server.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-algo-server/issues"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "bfx-hf-algo": "^2.0.0",
    "bfx-hf-models": "^2.1.0",
    "bfx-hf-util": "^1.0.1",
    "bitfinex-api-node": "^4.0.0",
    "bluebird": "^3.5.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0",
    "p-iteration": "^1.1.7",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "bfx-api-node-rest": "^3.0.16",
    "bfx-hf-ext-plugin-bitfinex": "^1.0.0",
    "bfx-hf-models-adapter-lowdb": "^1.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^27.0.4",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^7.2.0",
    "nyc": "^15.0.0",
    "socks-proxy-agent": "^5.0.0"
  }
}
